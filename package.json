{
	"private": true,
	"devDependencies": {
		"@parcel/transformer-sass": "^2.4.1",
		"del-cli": "^4.0.1",
		"parcel": "^2.4.1",
		"sass": "^1.49.11",
		"serve": "^13.0.2"
	},
	"scripts": {
		"compile:sass": "parcel build --no-source-maps --dist-dir public/assets/css src/sass/*.sass",
		"watch:sass": "parcel watch --no-source-maps --dist-dir public/assets/css src/sass/*.sass",
		"clean": "del public/assets/css public/assets/js",
		"serve": "serve public"
	}
}
